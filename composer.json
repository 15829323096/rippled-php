{
    "name": "mikemilano/xrphp",
    "description": "A PHP library for communication with the XRP Ledger.",
    "keywords": ["library", "composer", "package"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Milano",
            "email": "coder1@gmail.com"
        }
    ],
    "type": "project",
    "require": {
		"php": ">=7.1",
        "php-http/client-implementation": "^1.0",
        "php-http/client-common": "^1.7",
        "php-http/discovery": "^1.4",
        "php-http/httplug": "^1.1",
        "psr/http-message": "^1.0",
        "php-http/message-factory": "^1.0",
        "symfony/process": "^4.1"
    },
	"require-dev": {
        "phpunit/phpunit": "^7.1",
        "php-http/mock-client": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "php-coveralls/php-coveralls": "^2.1"
    },
	"autoload": {
        "psr-4": {
            "XRPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XRPHP\\Tests\\": "tests/"
        }
    }
}
